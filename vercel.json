{
  "buildCommand": "cd frontend && yarn install && yarn build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && yarn install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "$VITE_API_URL/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
